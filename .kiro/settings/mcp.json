{
  "mcpServers": {
    "intelligent-placeholder": {
      "command": "python",
      "args": ["mcp_tools/intelligent_placeholder_tools.py"],
      "env": {
        "PYTHONPATH": "backend:.",
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "disabled": false,
      "autoApprove": [
        "extract_placeholders",
        "validate_placeholders",
        "understand_placeholder_semantics",
        "generate_field_matching_suggestions",
        "generate_etl_instructions",
        "process_template_complete"
      ],
      "description": "智能占位符处理工具集，提供占位符解析、语义理解、字段匹配和ETL指令生成功能"
    }
  }
}